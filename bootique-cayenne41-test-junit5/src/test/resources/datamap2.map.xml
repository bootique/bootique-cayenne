<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/10/modelMap"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/10/modelMap http://cayenne.apache.org/schema/10/modelMap.xsd"
	 project-version="10">
	<property name="defaultPackage" value="io.bootique.cayenne.v41.test.persistence"/>
	<property name="quoteSqlIdentifiers" value="true"/>
	<db-entity name="table1">
		<db-attribute name="a" type="BIGINT"/>
		<db-attribute name="b" type="BIGINT"/>
		<db-attribute name="id" type="BIGINT" isPrimaryKey="true" isMandatory="true"/>
	</db-entity>
	<db-entity name="table2">
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="name" type="VARCHAR" length="20"/>
	</db-entity>
	<obj-entity name="Table1" className="io.bootique.cayenne.v41.test.persistence.Table1" dbEntityName="table1">
		<obj-attribute name="a" type="java.lang.Long" db-attribute-path="a"/>
		<obj-attribute name="b" type="java.lang.Long" db-attribute-path="b"/>
	</obj-entity>
	<obj-entity name="Table2" className="io.bootique.cayenne.v41.test.persistence.Table2" dbEntityName="table2">
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="name"/>
	</obj-entity>
</data-map>
